        [Conditions:Cyber-Condition-Ramming]

        [MaxActions:-1]
        [Actions:Cyber-Action-Ramming]

      </Description>

    </EntityComponent>

    <EntityComponent xsi:type="MyObjectBuilder_InventoryComponentDefinition">
      <Id>
        <TypeId>Inventory</TypeId>
        <SubtypeId>Cyber-Condition-Ramming</SubtypeId>
